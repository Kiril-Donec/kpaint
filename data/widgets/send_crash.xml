<gui>
<window id="send_crash" text="@.title">
  <vbox>
    <vbox id="official">
      <label text="@.send_file" />
      <link id="filename" text="" url="" />
      <label text="@.to_email" />
      <entry readonly="true" text="support@kpaint.org" maxsize="256" />
      <label text="@.explaining" />
    </vbox>
    <vbox id="dev">
