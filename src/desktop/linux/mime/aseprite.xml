<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="image/x-kpaint">
         <comment xml:lang="en">KPaint Pixel Art</comment>
         <magic priority="50">
                <match type="little16" value="0xA5E0" offset="4"/>
         </magic>
         <glob pattern="*.ase"/>
         <glob pattern="*.aseprite"/>
        <icon name="kpaint"/>
  </mime-type>
</mime-info>
